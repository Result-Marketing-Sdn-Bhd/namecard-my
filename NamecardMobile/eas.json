{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundlePlayDebug",
        "credentialsSource": "remote"
      },
      "ios": {
        "simulator": true,
        "credentialsSource": "remote"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundlePlayRelease",
        "credentialsSource": "remote",
        "autoIncrement": true
      },
      "ios": {
        "simulator": false,
        "credentialsSource": "remote"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "APP_ENV": "production"
      },
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundlePlayRelease",
        "credentialsSource": "remote",
        "autoIncrement": true
      },
      "ios": {
        "simulator": false,
        "credentialsSource": "remote",
        "autoIncrement": true
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production",
        "releaseStatus": "completed"
      },
      "ios": {
        "appleId": "ngsanzen@gmail.com",
        "ascAppId": "6754809694",
        "appleTeamId": "3WHF9353VV"
      }
    }
  }
}